[] -> Int zero;
[Int, Int] -> Int sum;
[Int] -> [Int] -> Int curriedSum;
zero = lambda () -> Int {
return 0;
};
sum = lambda (Int y, Int x) -> Int {
return x + y;
};
curriedSum = lambda (Int x) -> [Int] -> Int {
return lambda (Int y) -> Int {
return x + y;
};
};
zero();
sum(1, 2);
curriedSum(1)(2);
lambda (Int y, Int x) -> Bool {
return true;
}(1, 2);
1(2, 3);
