/*
Test declaring local variables inside lambdas.
RUN: %parser "%s" > "%t"
RUN: %diff "%s.expect" "%t"
*/
[Int] -> [Int] -> Int curriedSum;

curriedSum = lambda (Int x)->[Int] -> Int {
    Int arg;
    [Int] -> Int curried;
    arg = x;
    curried = lambda(Int y)->Int {
        return arg + y;
    };
    return curried;
};