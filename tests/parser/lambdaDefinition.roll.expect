[] -> Int zero;
[Int, Int] -> Int sum;
[Int] -> [Int] -> Int curriedSum;
[Int] -> Void printInt;
zero = lambda () -> Int {
return 0;
};
sum = lambda (Int y, Int x) -> Int {
return x + y;
};
curriedSum = lambda (Int x) -> [Int] -> Int {
return lambda (Int y) -> Int {
return x + y;
};
};
printInt = lambda (Int x) -> Void {
putChar(x);
};
